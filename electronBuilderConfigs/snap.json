{
  "extends": null,
  "appId": "io.github.myhpwa.twdi",
  "files": [
    "buildElectron/**/*",
    "package.json"
  ],
  "extraResources": [
    {
      "from": "electronBuilderConfigs",
      "to": ".",
      "filter": ["IsSnap.txt"]
    }
  ],
  "asarUnpack": [
    "node_modules/node1-libxmljsmt-myh/**/*"
  ],
  "icon": "buildElectron/icon.png",
  "snap": {
    "artifactName": "${name}_${version}_linux64.${ext}",
    "category": "public.app-category.education"
  }
}